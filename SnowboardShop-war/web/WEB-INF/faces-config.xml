<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
   <navigation-rule>
       <navigation-case>
        <from-view-id>/customerIndex.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
       <navigation-case>
        <from-view-id>/snowboards.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
       <navigation-case>
        <from-view-id>/boots.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
       <navigation-case>
        <from-view-id>/bindningar.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
       <navigation-case>
        <from-view-id>/productView.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
       <navigation-case>
        <from-view-id>/cart.xhtml</from-view-id>
                  <from-outcome>Logga ut</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/logout.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Logga ut</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
       <navigation-rule>
       <navigation-case>
        <from-view-id>/customerIndex.xhtml</from-view-id>
                  <from-outcome>korg</from-outcome>
            <to-view-id>/korg.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/customerIndex.xhtml</from-view-id>
        <navigation-case>  
            <from-outcome>show_details</from-outcome>
            <to-view-id>/productView.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>customer</from-outcome>
            <to-view-id>/customerIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
  <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>premium</from-outcome>
            <to-view-id>/customerIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    <navigation-rule>
        <from-view-id>/productView.xhtml</from-view-id>
        <navigation-case>  
            <from-outcome>return_index</from-outcome>
            <to-view-id>/customerIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>customer</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>customer</from-outcome>
            <to-view-id>/customerIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>premium</from-outcome>
            <to-view-id>/premiumCustomerIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
</faces-config>
